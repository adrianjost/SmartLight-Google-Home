{
	"name": "smartlight-api",
	"author": "Adrian Jost",
	"version": "0.0.1",
	"license": "MIT",
	"description": "SmartLight V3 API Cloud Functions",
	"engines": {
		"node": "10"
	},
	"scripts": {
		"lint": "run-s lint:*",
		"lint:eslint": "eslint . --fix",
		"lint:prettier": "prettier --write -c \"**/*.{js,json,md}\"",
		"serve": "firebase serve --only functions",
		"shell": "firebase experimental:functions:shell",
		"deploy:OAuth2UsersCreate": "firebase deploy --only functions:OAuth2UsersCreate",
		"deploy:OAuth2UsersDelete": "firebase deploy --only functions:OAuth2UsersDelete",
		"deploy:OAuth2/token": "firebase deploy --only functions:OAuth2/token",
		"deploy:OAuth2/authorize": "firebase deploy --only functions:OAuth2/authorize",
		"deploy:OAuth2/authentication": "firebase deploy --only functions:OAuth2/authentication",
		"deploy:GoogleHome": "firebase deploy --only functions:GoogleHome",
		"deploy": "run-p deploy:*"
	},
	"dependencies": {
		"camelcase": "^5.3.1",
		"color-name-list": "^4.11.0",
		"firebase-admin": "^8.9.0",
		"firebase-functions": "^3.3.0",
		"glob": "^7.1.6",
		"oauth2-firebase": "^0.1.30",
		"restana": "^4.0.3"
	},
	"devDependencies": {
		"eslint": "^6.8.0",
		"eslint-plugin-promise": "^4.2.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1",
		"yorkie": "^2.0.0"
	}
}
